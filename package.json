{
  "name": "vandelay_rental",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/courseSeed.js && node scripts/rentalSeed.js && node scripts/saleSeed.js && node scripts/categorySeed.js && node scripts/userSeed.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "connect-mongo": "^2.0.1",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "gridfs-stream": "^1.1.1",
    "hellosign-embedded": "^1.3.3",
    "hellosign-sdk": "^1.2.0",
    "if-env": "^1.0.4",
    "moment": "^2.22.2",
    "mongoose": "^5.0.17",
    "multer": "^1.3.0",
    "multer-gridfs-storage": "^3.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-calendar": "^2.15.2",
    "react-dom": "^16.4.1",
    "react-onclickoutside": "^6.7.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-table": "^6.8.6"
  }
}
